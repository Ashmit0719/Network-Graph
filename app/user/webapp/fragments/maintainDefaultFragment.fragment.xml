<core:FragmentDefinition 
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Dialog id="_IDGenDialog1" title="Maintain Default" afterClose="onCloseDetailsDialog">
        <VBox id="oib_update" visible="true" class="sapUiSmallMarginBeginEnd sapUiSmallMarginTop">
            <!-- Your form elements here -->
            <HBox id="hbox4">
                <VBox id="v7" width="75%">
                    <Label id="_IDGenLabel4" text="Material Temperature:"/>
                    <Input id="temp_input" placeholder="" liveChange="onLiveChange" value="" />
                </VBox>
                <VBox id="v8" class="sapUiSmallMarginBegin">
                    <Label id="deftemp_label" text="UoM:"/>
                    <Input id="defuom_input" value="" liveChange="onLiveChangeOnUom" editable="false"></Input>
                </VBox>
            </HBox>
            <!-- More form elements here -->
        </VBox>
        <beginButton>
            <Button id="_IDGenButton1" text="Confirm" type="Emphasized" press="onUpdateDefault" />
        </beginButton>
        <endButton>
            <Button id="_IDGenButton2" text="Close" icon="sap-icon://close-command-field" type="Reject" press="onCloseMiantainDialog" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
